{
  "name": "ht-component",
  "version": "0.0.7",
  "private": false,
  "description": "核桃组件库 - 基于vue3,bpmnjs的流程设计器",
  "keyword": "vue3 bpmnjs",
  "main": "dist/HtComponent.umd.js",
  "author": "langxue",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "cross-env NODE_ENV=production vue-cli-service build",
    "build:dev": "cross-env NODE_ENV=dev vue-cli-service build --mode dev",
    "build:test": "cross-env NODE_ENV=k8s vue-cli-service build --mode k8s",
    "build:pre": "cross-env NODE_ENV=pre vue-cli-service build --mode pre",
    "build:lib": "vue-cli-service build --target lib --name HtComponent src/views/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@bpmn-io/properties-panel": "0.19.0",
    "abrain-web-component": "0.0.9",
    "axios": "^0.27.2",
    "bpmn-js": "9.4.0",
    "bpmn-js-properties-panel": "^1.11.0",
    "camunda-bpmn-moddle": "4.5.0",
    "core-js": "^3.6.5",
    "element-plus": "^2.2.10",
    "screenfull": "4.2.0",
    "vue": "^3.2.37",
    "vue-router": "4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.14",
    "@vue/cli-plugin-eslint": "~4.5.14",
    "@vue/cli-service": "~4.5.14",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "ht-component": "0.0.7",
    "node-scss": "^7.0.3",
    "raw-loader": "^4.0.2",
    "sass-loader": "^13.1.0",
    "scss": "^0.2.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off",
      "no-unexpected-multiline": "off",
      "no-constant-condition": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
